/*
 * helloworld.c: simple test application
 *
 * This application configures UART 16550 to baud rate 9600.
 * PS7 UART (Zynq) is not initialized by this application, since
 * bootrom/bsp configures it to baud rate 115200
 *
 * ------------------------------------------------
 * | UART TYPE   BAUD RATE                        |
 * ------------------------------------------------
 *   uartns550   9600
 *   uartlite    Configurable only in HW design
 *   ps7_uart    115200 (configured by bootrom/bsp)
 */

#include <stdio.h>
#include "platform.h"
#include "led_k.h"
#include "mblaze_nt_types.h"
#include "usart.h"
#include "math.h"
#include "delay.h"
#include "i2c_software_master.h"




int main()
{
//	  assign sda_pin_T=(slv_reg0[31])?1:0;////if T==0 buf is connected to ouput  else is high impedance
//	  assign scl_pin_T=(slv_reg0[30])?1:0;
//
//	  assign sda_pin_O=0;
//	  assign scl_pin_O=0;
//
//	  assign slv_reg_temp[29]=sda_pin_I;
//	  assign slv_reg_temp[28]=scl_pin_I;
	i2c_init();
	while(1){
		turn_led(sda_read()|scl_read()<<1);
	}
//	u8 status;
//	i2c_master_write_simple(0xc0,22,0xf0);
//	delay_ms(10);
//	i2c_master_write_simple(0xc0,22,0xf5);
//	delay_ms(10);
//	i2c_master_write_simple(0xc0,22,0xf5);
//	delay_ms(10);
//	status=i2c_master_write_simple(0xc0,22,0xf5);
//	turn_led(status);
	return 0;
}



